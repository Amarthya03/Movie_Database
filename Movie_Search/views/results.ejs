<head>
    <title>Movies | Search Results</title>
    <link href="https://fonts.googleapis.com/css?family=Fredoka+One|Patua+One&display=swap" rel="stylesheet">
</head>
<style>
    ol {
        margin: 2% 30%;
        font-family: 'Patua One', cursive;
    }
    h1 {
        margin-top: 3% ;
        font-family: 'Fredoka One', cursive;
    }
    h2 {
        margin-top: 1%;
        font-family: 'Patua One', cursive;
    }
</style>
<h1><center>Results Page</center></h1>
<ol>
    <% data["Search"].forEach(function(movie){ %>
        <li><h2><%= movie["Title"] %> - <%= movie["Year"]%></h2></li>
    <% }) %>
</ol>
